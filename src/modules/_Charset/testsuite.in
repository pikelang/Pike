test_eq(_Charset.decoder ("utf-8")->feed ("\u00f4\u008f\u00bf\u00bf")->drain(), "\U0010ffff")
test_eval_error(return _Charset.decoder ("utf-8")->feed ("\u00f4\u0090\u0080\u0080")->drain())

test_eval_error(return _Charset.decoder ("utf-8")->feed ("\xc0\x80")->drain())
